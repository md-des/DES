{
  "name": "uhs-admin",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "start": "better-npm-run start-dev",
    "start:no-proxy": "cross-env NO_PROXY=true roadhog server",
    "build": "DISABLE_ESLINT=true roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "roadhog build --analyze",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "jest",
    "build:dll": "roadhog buildDll",
    "test:all": "node ./tests/run-tests.js",
    "pm2-start": "better-npm-run start-pm2"
  },
  "betterScripts": {
    "start-dev": {
      "command": "roadhog server",
      "env": {
        "APIHOST": "140.143.90.177",
        "APIPORT": 3060
      }
    },
    "start-pm2": {
      "command": "set NODE_ENV=production&&pm2 start ecosystem.json"
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "^0.1.0",
    "bizcharts": "^3.1.0-beta.2",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.2",
    "http-proxy-middleware": "^0.17.4",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-jest": "^21.0.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "gh-pages": "^1.0.0",
    "jest": "^21.0.1",
    "lint-staged": "^4.3.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.9.0",
    "pro-download": "^1.0.0",
    "roadhog": "^1.3.1",
    "roadhog-api-doc": "^0.3.3",
    "rollbar": "^2.3.1",
    "stylelint": "^8.1.0",
    "stylelint-config-standard": "^17.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test|e2e).js?(x)"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jasmine.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/styleMock.js"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "lint-staged:js",
    "**/*.less": "stylelint --syntax less",
    "*.{js,jsx,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
